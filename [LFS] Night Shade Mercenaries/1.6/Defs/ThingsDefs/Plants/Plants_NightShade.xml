<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ThingDef ParentName="CavePlantBase">
        <defName>Plant_RawDarvonishum</defName>
        <label>Darvonishum</label>
        <description>A meaty bioengineered mushroom grown for its nutritious value. It can grow on fungal gravel, but
            only in darkness - exposing it to any kind of light, including darklights, will kill it. They may take
            awhile to grow but can be eaten raw and offer huge amounts of Nutrition</description>
        <statBases>
            <MaxHitPoints>85</MaxHitPoints>
            <Nutrition>0.25</Nutrition>
        </statBases>
        <graphicData>
            <texPath>Things/Plants/Darvonishum</texPath>
            <graphicClass>Graphic_Random</graphicClass>
            <shadowData>
                <volume>(0.2, 0.3, 0.13)</volume>
                <offset>(-0.03,0,-0.44)</offset>
            </shadowData>
        </graphicData>
        <selectable>true</selectable>
        <pathCost>14</pathCost>
        <ingestible/>
        <plant>
            <fertilityMin>0.3</fertilityMin>
            <fertilitySensitivity>0.15</fertilitySensitivity>
            <harvestTag>Standard</harvestTag>
            <harvestedThingDef>RawDarvonishum</harvestedThingDef>
            <harvestYield>5</harvestYield>
            <sowTags>
                <li>Ground</li>
                <li>Hydroponic</li>
            </sowTags>
            <sowMinSkill>10</sowMinSkill>
            <sowResearchPrerequisites>
                <li>DarvonishumResearch</li>
            </sowResearchPrerequisites>
            <topWindExposure>0</topWindExposure>
            <growDays>15</growDays>
            <cavePlantWeight>0</cavePlantWeight>
            <visualSizeRange>0.3~0.65</visualSizeRange>
            <purpose>Food</purpose>
            <humanFoodPlant>true</humanFoodPlant>
            <maxMeshCount>9</maxMeshCount>
        </plant>
    </ThingDef>
</Defs>